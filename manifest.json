{
  "manifest_version": 2,
  "name": "HoogleSwitcher",
  "description": "Help haskellers search hackage and stackage",
  "version": "1.0",
  "icons": {
    "16": "images/icons/icon-16.png",
    "48": "images/icons/icon-48.png",
    "128": "images/icons/icon-128.png"
  },
  "permissions": [
    "storage"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icons/icon-19.png",
      "38": "images/icons/icon-38.png"
    },
    "default_title": "HoogleSwitcher",
    "default_popup": "popup.html"
  }
}
