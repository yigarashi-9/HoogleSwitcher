{
  "manifest_version": 2,
  "version": "0.0.0.3",
  "name": "HoogleSwitcher",
  "author": "Yuu Igarashi",
  "description": "Help haskellers search hackage and stackage",
  "icons": {
    "16": "src/images/icons/icon-16.png",
    "48": "src/images/icons/icon-48.png",
    "128": "src/images/icons/icon-128.png"
  },
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_icon": {
      "19": "src/images/icons/icon-19.png",
      "38": "src/images/icons/icon-38.png"
    },
    "default_title": "HoogleSwitcher",
    "default_popup": "src/popup.html"
  },
  "web_accessible_resources": [
    "src/options.html",
    "src/js/options.js",
    "src/css/main.css"
  ],
  "options_page": "src/options.html"
}
